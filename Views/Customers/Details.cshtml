@model Movie_Rental_Store.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer</h2>

<h2>@Model.Name</h2>

<ul>
    <li>Membership Type : @Model.MembershipType.Name</li>
    @if(Model.BirthDate != null)
    {
        <li>Date of Birth : @Model.BirthDate.Value.ToShortDateString()</li>
    }
</ul>